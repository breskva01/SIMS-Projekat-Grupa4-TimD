<Window x:Class="InitialProject.View.OwnerView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View"
        mc:Ignorable="d"
        Title="OwnerView" Height="250" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Height="40" Width="170" Content="Register new accommodation" Click="ShowRegistrationAccommodationView_Click"></Button>
        <Button Grid.Row="1" Height="40" Width="170" Content="Rate guests" Click="ShowGuestRatingView_Click"></Button>
    </Grid>
</Window>
