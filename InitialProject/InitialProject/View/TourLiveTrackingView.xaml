<Window x:Class="InitialProject.View.TourLiveTrackingView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View"
        mc:Ignorable="d"
        Title="TourLiveTrackingView" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="keyPointsDataGrid" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridCheckBoxColumn Header="Country" Binding="{Binding Location.Country}"/>
                <DataGridCheckBoxColumn Header="City" Binding="{Binding Location.City}"/>
                <DataGridCheckBoxColumn Header="Place" Binding="{Binding Attraction}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
